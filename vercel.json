{
  "version": 2,
  "builds": [
    {
      "src": "frontend/srcpackage.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}
